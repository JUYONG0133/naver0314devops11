<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


    <bean id="per" class="study.ex3.Person">
        <constructor-arg value="손미나"/>
        <property name="phone" value="010-203-123"/>
    </bean>

    <bean id="cart" class="study.ex3.Mycart">
        <constructor-arg value="상의"/>
        <constructor-arg value="23000"/>
        <property name="person" ref="per"/>
    </bean>


</beans>