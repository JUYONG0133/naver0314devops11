package day0318;

public class Ex09_For {

	public static void main(String[] args) {
		
		for(int i = 1; i<=10; i++) {
			System.out.printf("%4d",i);
			}
		System.out.println();
		
		for(int i = 10; i>=1; i--) {
			System.out.printf("%4d",i);
			}
		System.out.println();
		
		for(int i = 1; i<=10; i+=3) {
			System.out.printf("%4d",i);
			}
		System.out.println();
		
		for(char i = 'A'; i<='Z'; i++) {
			System.out.printf("%4c",i);
			}
		System.out.println();
		System.out.println();
		for(int i = 65 ; i<= 90; i++) {
			System.out.printf("%4c",i);
			}
		System.out.println();
		
		
		
		
		
		
	
		
		
		
	}

}
