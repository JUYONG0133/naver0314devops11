package day0325;

//상속
class AA{ //BB의 부모 클래스
	AA(){
		System.out.println("AA의 디폴트 생성자");
	}
}

class BB extends AA{   //class aa 를 상속받는 class bb  AA의 자식 클래스
	BB(){
		System.out.println("BB의 디폴트 생성자");
	}
}



public class Ex04_Inheri {

	public static void main(String[] args) {
		
		BB b = new BB();
		
	}

}
