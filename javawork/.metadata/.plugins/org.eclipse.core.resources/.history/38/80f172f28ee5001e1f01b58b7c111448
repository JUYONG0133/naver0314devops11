package day0319;

public class test1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		System.out.println("1~100까지의 소수");
		int count=0;
		for(int i = 2; i <= 100; i++) {
			for(int j = 2; j <= i; j++) {
				if(i%j==0) {
					if(j==i) {
						System.out.println(i);
					}
					else break;
				}
				count ++;
			}
			

		}
		System.out.println();
		System.out.println(count);
	}

}
