package day0319;

public class test1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		System.out.println("1~100까지의 소수");
		int count=0;
		for(int i = 1; i <= 100; i++) {
			for(int j = i; j <= 100; j++) {
				if((j%i!=0)&&(j%i==1)) {
					System.out.println(j);
				}
			}
			//if(i)

		}
		System.out.println();
		System.out.println(count);
	}

}
